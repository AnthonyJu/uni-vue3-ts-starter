    "@types/weixin-app": "^2.9.7",
    "@unocss/eslint-config": "^0.63.6",
    "@vue/language-server": "^2.1.10",
    "@vue/runtime-core": "^3.5.12",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "sass": "^1.77.6",
    "sass-loader": "^16.0.3",
